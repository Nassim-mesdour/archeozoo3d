{
  "name": "myapp",
  "version": "1.3.1",
  "private": true,
  "productName": "archeozoo3D",
  "main": "electron.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager ./ --overwrite --platform=darwin --arch=x64 --icon=public/assets/images/icon.icnc --prune=true --out=release-builds",
    "package-win": "electron-packager ./ archeozoo3D --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/assets/images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Archeozoo3D\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "nodemon": "^1.18.9"
  },
  "devDependencies": {
    "electron": "^4.0.2",
    "electron-packager": "^13.0.1"
  }
}
